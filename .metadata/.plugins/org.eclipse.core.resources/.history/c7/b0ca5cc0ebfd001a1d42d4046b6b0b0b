package com.tianliyuan.task;

public class UniqueTest {
	public static void main(String[] args) {
		Boolean isUnique = true;
		int[] list = new int[6];
		int index = 0;
		int random;
		do {
			random = (int)Math.random() * 31;
			if (index == 0) {
				list[index] = random;
			} else {
				for(int i = 0; i < index; i++) {
					if(list[i] == random) {
						isUnique = false;
					}
				}
			}
			if(isUnique) {
				list[index] = random;
				index++;
			}
		} while (!isUnique == true && index < list.length);
	}
}
